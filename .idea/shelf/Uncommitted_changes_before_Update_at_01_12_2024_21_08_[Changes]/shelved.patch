+{% extends 'layout.html' %}
+
+{% block title %}
+    <title>Ajouter une Inscription</title>
+{% endblock %}
+
+{% block body %}
+  <h2>Ajouter une Inscription</h2>
+  <form method="post" action="/inscription/add">
+    <fieldset>
+        <legend>Créer une Inscription</legend>
+
+        <label for="nom">Nom</label><br>
+        <input id="nom" type="text" name="nom" size="50" value="" placeholder="Saisir le nom" required><br>
+
+        <label for="email">Email</label><br>
+        <input id="email" type="email" name="email" size="50" value="" placeholder="Saisir l'email" required><br>
+
+        <label for="date_inscription">Date d'inscription</label><br>
+        <input id="date_inscription" type="date" name="date_inscription" required><br>
+
+        <input type="submit" value="Ajouter" name="Valider">
+    </fieldset>
+  </form>
+{% endblock %}
